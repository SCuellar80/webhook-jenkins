
pipeline {
    agent any
    stages {
        stage('Git clone') 
        {
            steps 
            {
                git branch: 'main', poll: false, url: 'https://github.com/SCuellar80/webhook-test.git'
            }
        }
        stage('Build') {
                steps {
                    echo 'Building..'
                }
            }
            stage('Test') {
                steps {
                    echo 'Testing..'
                }
            }
            stage('Deploy') {
                steps {
                    echo 'Deploying....'
                }
            }
    }
}
